              <h4>Loan calculator</h4>
              <form action="loan" method="get" class="d-flex flex-column">
                <div class="mb-3">
                  <label class="label" for="loandetails">Type of Loan</label>
                  <select class="w-100" id="loandetails">
                    <option value="">Types of Loan</option>
                    <option value="Debt-Financing">Debt Loan</option>
                    <option value="Housing-Finance">Housing Loan</option>
                    <option value="Education-Finance">Education Loan</option>
                    <option value="Car-Finance">Car Loan</option>
                    <option value="Business-Finance">Business Loan</option>
                    <option value="Other">Other</option>
                  </select>
                </div>
                <div class="mb-3">
                  <label class="label" for="loan-amount">Amount of money
                  </label>
                  <div class="input-field">
                    <span>&#8377;</span>
                    <input required="true" type="number" id="loan-amount" placeholder="Enter amount"
                      class="form-control" />
                  </div>
                </div>
                <!-- Tenure Period -->
                <div class="mb-3">
                  <label class="label" for="loan-time">Tenure Period</label>
                  <div class="input-field tenure">
                    <input required type="number" id="loan-time" placeholder="Tenure Period" class="form-control" />
                    <select class="w-100" id="loandetails01">
                      <option value="">Year</option>
                      <option value="Month">Months</option>
                    </select>
                  </div>
                </div>
                <!-- Rate of Intrest -->
