            <!-- Loan calculator -->
            <div class="col-lg-6">
              <div
                class="basic-loan-calculator wow fadeInLeft"
                data-wow-delay="0.1s"
              >
                <img class="shape" src="img/home-4/shape-7.png" alt="" />
                <h4 class="text-center">Loan calculator</h4>
                <form class="d-flex flex-column">
                  <div class="mb-3">
                    <label class="label" for="loandetails">Type of Loan</label>
                    <select class="w-100" id="loandetails" required>
                      <option value="">Types of Loan</option>
                      <!-- <option value="Debt-Financing">Debt Loan</option> -->
                      <% loanCategories.forEach(element => { %>
                      <option value="<%= element %>"><%= element %></option>
                      <% }) %>
                    </select>
                  </div>
                  <div class="mb-3">
                    <label class="label" for="loan-amount"
                      >Amount of money
                    </label>
                    <div class="input-field">
                      <span>&#8377;</span>
                      <input
                        required="true"
                        type="number"
                        id="loan-amount"
                        placeholder="Enter amount"
                        class="form-control"
                      />
                    </div>
                  </div>
                  <!-- Tenure Period -->
                  <div class="mb-3">
                    <label class="label" for="loan-time">Tenure Period</label>
                    <div class="input-field tenure">
                      <input
                        required
                        type="number"
                        id="loan-time"
                        placeholder="Tenure Period"
                        class="form-control"
                      />
                      <select class="w-100 nice-select" id="loandetails">
                        <option value="">Year</option>
                        <option value=" month">Month</option>
                      </select>
                    </div>
                  </div>
                  <!-- Rate of Intrest -->
                  <div class="mb-3">
                    <label class="label" for="rate-interest"
                      >Rate of Interest
                    </label>
                    <div class="input-field">
                      <span>&#37;</span>
                      <input
                        required="true"
                        type="number"
                        id="rate-interest"
                        placeholder="Enter Rate of Interest"
                        class="form-control"
                      />
                    </div>
                  </div>

                  <button
                    type="button"
                    class="theme-btn w-100"
                    id="calculateButton"
                  >
                    Calculate
                  </button>
                </form>
              </div>
            </div>
            <!-- Modal -->
            <div id="loanResultModal" class="modal homepage">
