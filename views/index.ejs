      <!-- what client says section -->
      <section class="client-area-2 pt-60 pb-60">
        <div class="container">
          <div class="section-title">
            <span class="short-title-2">_Testimonials</span>
            <h1 class="wow fadeInUp mb-3">
              Customer <span class="underline-shape">feedbacks</span>
            </h1>
            <p class="wow fadeInUp" data-wow-delay="0.1s">
              Testimonials Unleashed: Hear Our Customers Speak.
            </p>
          </div>

          <div class="client-slider-2">
            <% feedbacks.forEach((feedback, index)=> { %>
            <div
              class="single-client wow fadeInUp"
              data-wow-delay="<%= (index % 3) * 0.2 %>s"
            >
              <!-- star section  To be added later -->
              <!-- <div class="rating">
                <% for (let i=0; i < 5; i++) { %>
                <a href="#"><i class="icon_star"></i></a>
                <% } %>
              </div> -->
              <p class="quote"><%= feedback.message %></p>

              <div class="client-info">
                <div>
                  <p><%= feedback.fullname %></p>
                  <span class="role"> <%= feedback.subject %> </span>
                </div>
              </div>
            </div>
            <% }); %>
          </div>

          <!-- pop-up html code -->
          <div class="lightbox" id="lightbox">
            <div class="popup-container">
              <div class="text-dark d-flex justify-content-end">
                <button
                  type="button"
                  class="close close-btn border border-secondary p-1"
                  aria-label="Close"
                >
                  <span aria-hidden="true ">&times;</span>
                </button>
              </div>

              <form
                action="/feedback"
                method="post"
                enctype="multipart/form-data"
                class="feedback-form-widget"
              >
                <h3 class="text-dark">Feedback Form</h3>
                <div class="row">
                  <div class="col-md-6 feedback-field">
                    <label for="f-name text-start">Full Name</label>
                    <input
                      type="text"
                      id="f-name"
                      name="fullname"
                      class="form-control"
                      placeholder="What's your name?"
                      required
                    />
                  </div>
                  <div class="col-md-6 feedback-field">
                    <label for="email-address">Email</label>
                    <input
                      type="email"
                      id="email-address"
                      name="email"
                      class="form-control"
                      placeholder="yourname@example.com"
                      required
                    />
                  </div>
                  <div class="col-md-12 mt-md-0 feedback-field">
                    <label for="subject">Subject(50 Character Max)</label>
                    <input
                      type="text"
                      id="subject"
                      name="subject"
                      maxlength="50"
                      class="form-control"
                      placeholder="Enter Subject"
                      required
                    />
                  </div>

                  <div class="col-md-12 feedback-field">
                    <label for="form-text">Messages (150 Character Max)</label>
                    <textarea
                      cols="20"
                      rows="3"
                      name="message"
                      id="form-text"
                      maxlength="150"
                      class="form-control pt-15"
                      placeholder="Your message......"
                      required
                    ></textarea>
                  </div>

                  <div class="col-12 mt-35">
                    <button type="submit" class="theme-btn theme-btn-lg w-100">
                      Submit Now
                    </button>
                  </div>
                </div>
              </form>
            </div>
          </div>
          <!-- pop-up feedback code -->
          <div class="d-flex justify-content-center m-5">
            <button class="pop-btn btn btn-primary">Add Feedback</button>
          </div>
        </div>
      </section>
      <!-- what client says section end -->

