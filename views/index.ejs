      <!-- Frequently Ask Questions section -->
      <section class="faq-area-3">
        <div class="container">
          <div class="row gy-lg-0 gy-4">
            <div
              class="col-lg-6"
              style="border-radius: 15px; object-fit: cover; overflow: hidden"
            >
              <img
                style="height: 100%; width: 100%"
                class="faq-img img-fluid"
                src="img/poster/faq.png"
                alt="FAQ-Image"
              />
            </div>
            <div class="col-lg-6 pl-lg-50">
              <span class="subtitle">_FAQ</span>
              <h1>
                Frequently asked general
                <span class="underline-shape">questions</span>
              </h1>

              <div class="faq-widget">
                <div class="accordion" id="accordionExample">
                  <% frequently.slice(0, 3).forEach((faq, index)=> { %>
                  <div class="single-faq wow fadeInUp" data-wow-delay="0.1s">
                    <div class="w-100">
                      <div class="faq-header" id="heading<%= index %>">
                        <h6
                          class="mb-0 text-white"
                          data-bs-toggle="collapse"
                          data-bs-target="#collapse<%= index %>"
                          aria-expanded="true"
                          aria-controls="collapse<%= index %>"
                        >
                          <%= faq.question %>
                          <i class="icon_plus"></i>
                          <i class="icon_minus-06"></i>
                        </h6>
                      </div>
                      <div
                        id="collapse<%= index %>"
                        class="collapse show"
                        aria-labelledby="heading<%= index %>"
                        data-bs-parent="#accordionExample"
                      >
                        <div class="faq-body">
                          <p><%= faq.answer %></p>
                        </div>
                      </div>
                    </div>
                  </div>
                  <% }) %>
                </div>

                <h6 class="more-contact">
                  Have more question ? <a href="/contact">Contact Us</a>
                </h6>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- Frequently Ask Questions section -->

