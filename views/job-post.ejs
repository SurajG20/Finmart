                    <option value="SortBy Popular">SortBy Popular</option>
                    <option value="SortBy Trending">SortBy Trending</option>
                  </select>
                </div> -->
              </div>
              <% jobs.forEach(element=> { %>

                <div class="single-job-post me-1 wow fadeInUp mt-25 card card-body">
                  <div class="post-header">
                    <div>
                      <h6 class="job-title">
                        <a href="job-application/<%= element._id %>">
                          <%= element.title %> | <%= element.category %>
                        </a>
                      </h6>
                      <div class="d-flex flex-wrap">
                        <div class="job-location me-lg-3 me-2">
                          <i class="icon_pin_alt"></i>
                          <%= element.location %>
                        </div>
                        <div class="job-catagory">
                          <span>
                            <%= element.jobPosition %>
                          </span> | <%= element.jobType %>
                        </div>
                      </div>
                    </div>
                    <div class="timestamp">
                      <%= element.postedOn.toLocaleString() %>
                    </div>
                  </div>
                  <div class="post-content">
                    <p>
                      <%= element.description.substring(0,100) %>...
                    </p>
                  </div>
                </div>
                <% }) %>
            </div>

            <div class="email-alert-widget mt-100 wow fadeInUp">
              <h4>Get email alerts for the latest Jobs in India</h4>
              <p>You can cancel email alerts at any time.</p>

              <form id="subscribe-form">
