  <body>
    <!-- Preloader -->
    <div id="preloader">
      <div id="ctn-preloader" class="ctn-preloader">
        <div class="round_spinner">
          <div class="spinner"></div>
          <div class="text">
            <img src="img/Vinayak/9.jpg" height="100px" alt="" />
          </div>
        </div>
        <h2 class="head">Did You Know?</h2>
        <p></p>
      </div>
    </div>

    <!-- Header -->
    <header class="header">
      <div class="header-top py-2">
        <div class="container">
          <div class="row align-items-center">
            <div class="col-md-5">
              <div class="header-info-left">
                <!-- <div class="language-list">
                <select name="page-language-list" id="select-lang">
                  <option value="English">English</option>
                  <option value="Hindi">Hindi</option>
                </select>
              </div> -->

                <div class="timestamp ms-4">
                  <i class="icon_clock_alt"></i> 24 X 7 Available
                </div>
              </div>
            </div>
            <div class="col-md-7">
              <div class="header-info-right">
                <ul>
                  <li>
                    <img
                      class="img-fluid"
                      src="img/phone-outline-white.png"
                      alt="Phone-Image"
                    /><a href="tel:+91-9815353035">+91-9815353035</a>
                  </li>

                  <li>
                    <i class="icon_mail_alt"></i
                    ><a href="mailto:info@vinayakfinmart.com"
                      >info@vinayakfinmart.com</a
                    >
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="header-menu header-menu-2 bg_white" id="sticky">
        <nav class="navbar navbar-expand-lg">
          <div class="container">
            <a class="navbar-brand" href="/">
              <img id="navbar-logo" height="100px" alt="Vinayak-Image" />
            </a>
            <button
              class="navbar-toggler collapsed"
              type="button"
              data-bs-toggle="collapse"
              data-bs-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <span class="menu_toggle">
                <span class="hamburger">
                  <span></span>
                  <span></span>
                  <span></span>
                </span>
                <span class="hamburger-cross">
                  <span></span>
                  <span></span>
                </span>
              </span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav menu ms-auto">
                <li class="nav-item dropdown submenu">
                  <a
                    href="/"
                    class="nav-link dropdown-toggle"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                    >Home</a
                  >
                </li>
                <li class="nav-item dropdown submenu">
                  <a
                    class="nav-link dropdown-toggle"
                    href="/about"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                  >
                    About Us
                  </a>
                </li>
                <li class="nav-item dropdown submenu active">
                  <a
                    class="nav-link dropdown-toggle"
                    href="/loan"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                  >
                    Loan
                  </a>
                </li>
                <li class="nav-item dropdown submenu">
                  <a
                    class="nav-link dropdown-toggle"
                    href="/job-post"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                  >
                    Jobs
                  </a>
                </li>

                <li class="nav-item dropdown submenu">
                  <a
                    class="nav-link dropdown-toggle"
                    href="/contact"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                  >
                    Contact Us
                  </a>
                </li>
                <li class="nav-item dropdown submenu">
                  <a
                    class="nav-link dropdown-toggle"
                    href="/blog"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                  >
                    Blog
                  </a>
                </li>
              </ul>
              <!-- dashboard button -->
              <% if (user && user?.isAdmin) { %>
              <a class="theme-btn theme-btn-outlined" href="/admin">
                Dashboard
              </a>
              <% } else if (user && !user?.isAdmin){ %>
              <a class="theme-btn theme-btn-outlined" href="/user">
                Dashboard
              </a>
              <% } else{ %>
              <a class="theme-btn theme-btn-outlined" href="/login">
                Dashboard
              </a>
              <% } %>

              <!-- <li class="nav-item dropdown submenu">
                <div class="px-2 js-darkmode-btn" title="Toggle dark mode">
